{
  "name": "@cliqz/user-agent-ios",
  "version": "3.0.0",
  "description": "Cliqz User Agent for iOS",
  "scripts": {
    "build-detox": "detox build",
    "build-user-scripts": "webpack --config webpack.config.js",
    "test-detox": "detox test --take-screenshots all",
    "postinstall": "sed -i -e 's/import \"sqlite3\\.h\"/import <sqlite3\\.h>/g' node_modules/react-native-sqlite-2/ios/RNSqlite2.m"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla-mobile/firefox-ios.git"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "@cliqz/component-ui-logo": "^0.4.0",
    "@cliqz/indexeddbshim": "^4.1.2",
    "browser-core-user-agent-ios": "https://s3.amazonaws.com/cdncliqz/update/edge/user-agent-ios/master/3.40.0.2735136.tgz",
    "cliqz-logo-database": "^0.4.0",
    "html-entities": "^1.2.1",
    "page-metadata-parser": "1.1.4",
    "react": "^16.8.3",
    "react-native": "0.59.10",
    "react-native-fs": "^2.14.1",
    "react-native-sqlite-2": "^2.0.3",
    "react-native-super-grid": "^3.0.9",
    "readability": "mozilla/readability#b9f47bcc8d3c223cabe2dec6a42eeb3bd778d85c",
    "tldts": "^5.0.3",
    "ws": ">=3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "detox": "^14.0.3",
    "glob": "^7.1.3",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.6"
  },
  "detox": {
    "configurations": {
      "sim": {
        "binaryPath": "builds/Build/Products/Release-iphonesimulator/UserAgent.app",
        "build": "xcodebuild -workspace UserAgent.xcworkspace -scheme 'Cliqz' -configuration Release -sdk iphonesimulator -derivedDataPath builds",
        "type": "ios.simulator",
        "name": "iPhone Xs"
      }
    },
    "test-runner": "mocha"
  }
}
